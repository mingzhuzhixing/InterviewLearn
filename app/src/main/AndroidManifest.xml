<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.v.interviewlearn">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:screenOrientation="portrait"
        tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.v.config_changes.ConfigChangesActivity"
            android:configChanges="orientation" />
        <activity android:name="com.v.active_proxy.ActiveProxyActivity" />
        <activity android:name="com.v.hook.start_no_register_activity.TestHookRegisteredActivity" />
        <activity android:name="com.v.hook.start_no_register_activity.TestHookActivity" />
        <activity android:name="com.v.service.messenger.DoubleMessengerServiceActivity" />
        <activity android:name="com.v.service.messenger.MessengerServiceActivity" />
        <activity android:name="com.v.service.aidl_service.AidlServiceActivity" />
        <activity android:name="com.v.hook.HookActivity" />
        <activity android:name="com.v.modularization.ModularActivity" />
        <activity android:name="com.v.service.base_service.ServiceActivity" />
        <activity android:name="com.v.database.DatabaseActivity" />
        <activity android:name="com.v.plugin.PluginActivity" />
        <activity android:name="com.v.ffmpeg.FFmpegActivity" />
        <activity android:name="com.v.mvp.Mvp3Activity" />
        <activity android:name="com.v.mvp.MvpActivity" />
        <activity android:name="com.v.event_dispatch.EventDispatchActivity" />
        <activity android:name="com.v.event_dispatch.EventDispatch2Activity" />
        <activity android:name="com.v.fragment_interface.FragmentMainActivity" />
        <activity android:name="com.v.ioc.IocActivity" />
        <activity android:name="com.v.eventbus.EventBusMainActivity" />
        <activity android:name="com.v.eventbus.EventBusSecondActivity" />

        <service android:name="com.v.service.base_service.MyService" />

        <activity android:name="com.v.glide.GlideActivity" />
        <activity android:name="com.v.async_task.AsyncTaskActivity"/>
        <activity android:name="com.v.webview.WebViewActivity"/>
    </application>

</manifest>